                            <option value="Lakshadweep">Lakshadweep</option>
                            <option value="Delhi">Delhi</option>
                            <option value="Puducherry">Puducherry</option>
                        </select>
                        <input type="text" name="pin" id="" placeholder="pin Code">
                        
                        <input type="password" name="password" id="" placeholder="Set password">
                        <input type="submit" class="button" value="Create My Account" name="" id="">
                    </form>
                   
