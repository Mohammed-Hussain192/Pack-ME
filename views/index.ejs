        <div class="second">
            <div class="inner2">
               
                <form novalidate action="/login" method="post">
                    <p class="cre2">Login Your Account</p>
                    <input type="email" name="email" id="" placeholder="Email">
                    <input type="password" name="password" id="" placeholder="Password">
                    <a class="forget" href="/forgetpassword">forget password?</a>
                    <input type="submit" value="Login" class="button2" name="" id="">
                </form>
            </div>
        </div>
